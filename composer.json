{
    "name": "activerules/env_from_s3",
    "description": "Load process.env variables from an S3 files securely.",
    "keywords": [
        "AWS",
        "ENV",
        "S3",
        "credentials",
        "secure"
    ],
    "homepage": "https://github.com/bwinkers/env-from-s3-php",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "aws/aws-sdk-php": "^3.61"
    },
    "require-dev": {
        "tracy/tracy": "^2.4",
        "nette/tester": "^2.0",
        "squizlabs/php_codesniffer": "^3.1.1",
        "phpunit/phpunit": "^6.4",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Activerules\\EnvFromS3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Activerules\\EnvFromS3\\Test\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Winkers",
            "email": "bwinkers@gmail.com"
        }
    ],
    "version": "0.0.1"
}